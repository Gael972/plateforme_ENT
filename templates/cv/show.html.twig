{% extends 'base.html.twig' %}

{% block title %}Cv{% endblock %}

{% block body %}
    <h1>Cv</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cv.id }}</td>
            </tr>
            <tr>
                <th>Intitule</th>
                <td>{{ cv.intitule }}</td>
            </tr>
            <tr>
                <th>DateD</th>
                <td>{{ cv.dateD ? cv.dateD|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateF</th>
                <td>{{ cv.dateF ? cv.dateF|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Photos</th>
                <td>{{ cv.photos }}</td>
            </tr>
            <tr>
                <th>Evaluation</th>
                <td>{{ cv.evaluation }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('cv_index') }}">back to list</a>

    <a href="{{ path('cv_edit', {'id': cv.id}) }}">edit</a>

    {{ include('cv/_delete_form.html.twig') }}
{% endblock %}
